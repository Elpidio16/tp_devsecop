pipeline {

    agent { label 'linux' } // Correction de 'étiquette' en 'label'

    options {
        buildDiscarder(logRotator(numToKeepStr: '5')) // Correction des parenthèses
    }

    stages {
        stage('Scan') {
            steps {
                withSonarQubeEnv('sql1') { // Utilisation correcte de l'environnement SonarQube
                    sh './mvnw clean org.sonarsource.scanner.maven:sonar-maven-plugin:3.9.0.2155:sonar' // Chemin sans espaces inattendus
                }
            }
        }
    }
}
